1.12
log
@feat: fake simplemove instead of easymotion
@
text
@" Modeline and Notes {
" vim: set sw=4 ts=4 sts=4 et tw=78 foldmarker={,} foldlevel=0 foldmethod=marker :
" }

" config for third plugin
function s:has_plug(nam)
  return isdirectory(expand(g:mvcf_path."pack/*/start/".a:nam)) + isdirectory(expand(g:mvcf_path."pack/*/opt/".a:nam))
endfunction

if s:has_plug('vim-easycomplete')
  ApcDisable
endif

if filereadable(expand(g:mvcf_path.'ext.vim.local'))
  execute 'source '.g:mvcf_path.'ext.vim.local'
endif

@


1.11
log
@fix: move easymotion to switch
@
text
@d10 2
a11 14
if s:has_plug('vim-easymotion')
  nmap     <leader>s <Plug>(easymotion-overwin-f2)
  nmap     <leader>wb <Plug>(easymotion-b)
  nmap     <leader>ww <Plug>(easymotion-w)
  " Disable default mappings
  let g:EasyMotion_do_mapping = 0
  " Turn on case-insensitive feature
  let g:EasyMotion_smartcase = 1
  hi link EasyMotionTarget ErrorMsg
  hi link EasyMotionShade  Comment
endif

if !s:has_plug('vim-easycomplete')
  echomsg "no easycomplete use ApcEnable" | ApcEnable
@


