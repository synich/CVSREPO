@<?php
function print_error($str){
  if (LOG_LEVEL && LOG_LEVEL>=1) {
    error_log($_SERVER['PATH_INFO'].' '.$str);
  }
}

function print_warn($str){
  if (LOG_LEVEL && LOG_LEVEL>=2) {
    error_log($_SERVER['PATH_INFO'].' '.$str);
  }
}

function print_info($str){
  if (LOG_LEVEL && LOG_LEVEL>=3) {
    error_log($_SERVER['PATH_INFO'].' '.$str);
  }
}

?>
