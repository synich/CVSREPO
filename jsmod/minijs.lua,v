1.2
log
@fix: minijs can deal tab
@
text
@function minijs(fname)
  local fin = io.open(fname, "r")
  local oname = fname:gsub(".js$", ".min.js")
  local fout = io.open(oname, "w")
  for li in fin:lines() do
    li = li:gsub("//.*", "")
    li = li:gsub("^[ \t]+", "")
    fout:write(li)
  end
  fin:close()
  fout:close()
end

minijs("sep.js")
@


1.1
log
@feat: minijs appear
@
text
@d1 14
a14 14
function minijs(fname)
  local fin = io.open(fname, "r")
  local oname = fname:gsub(".js$", ".min.js")
  local fout = io.open(oname, "w")
  for li in fin:lines() do
    li = li:gsub("//.*", "")
    li = li:gsub("^ +", "")
    fout:write(li)
  end
  fin:close()
  fout:close()
end

minijs("sep.js")
@

